<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/new_bg2x" >

        <include layout="@layout/top_bar" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@drawable/bg_22x" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="52dip"
            android:background="@drawable/jjjp_22x" />

        <ImageView
            android:id="@+id/homeImageView"
            android:layout_width="352dp"
            android:layout_height="160dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="80dip"
            android:focusable="true" />

        <ImageView
            android:id="@+id/homeImageView2"
            android:layout_width="352dp"
            android:layout_height="160dp"
            android:layout_marginLeft="20dip"
            android:layout_marginTop="80dip"
            android:focusable="true"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/paizi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="52dip"
            android:src="@drawable/home_imagebounds" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="182dip"
            android:background="@drawable/jjjp_12x" />

        <RelativeLayout
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="200dip" >

            <TextView
                android:id="@+id/city"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:text="常州"
                android:textColor="@color/white"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/weather"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentRight="true" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/city"
                android:background="?android:attr/listDivider" />

            <TextView
                android:id="@+id/temp"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/city"
                android:layout_marginLeft="5dp"
                android:textColor="@color/white"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="300dip"
            android:layout_below="@id/paizi"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp" >

            <ImageView
                android:id="@+id/yygl"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:src="@drawable/home_yyxl2x" />

            <ImageView
                android:id="@+id/tstj"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_alignLeft="@id/yygl"
                android:layout_marginLeft="80dp"
                android:layout_marginTop="60dp"
                android:src="@drawable/home_tese2x" />

            <ImageView
                android:id="@+id/jtlx"
                android:layout_width="100dp"
                android:layout_height="88dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="70dp"
                android:src="@drawable/home_jiaotong2x" />

            <ImageView
                android:id="@+id/yysj"
                android:layout_width="120dp"
                android:layout_height="100dp"
                android:layout_alignLeft="@id/yygl"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="110dp"
                android:src="@drawable/home_yysj2x" />

            <ImageView
                android:id="@+id/jjzl"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="150dp"
                android:src="@drawable/home_showway2x"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/zxgp"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_alignLeft="@id/jtlx"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="160dp"
                android:src="@drawable/home_xxgnk2x" />
        </RelativeLayout>
    </RelativeLayout>

    <include layout="@layout/bottom_bar" />

    <RelativeLayout
        android:id="@+id/level2"
        android:layout_width="240dip"
        android:layout_height="155dip"
        android:layout_gravity="bottom|center" >

        <ImageButton
            android:id="@+id/c1"
            android:layout_width="80dip"
            android:layout_height="88dip"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="2dip"
            android:background="@drawable/zx_select" />

        <ImageButton
            android:id="@+id/c2"
            android:layout_width="88dip"
            android:layout_height="86dip"
            android:layout_marginLeft="19dip"
            android:layout_marginTop="16dip"
            android:background="@drawable/gl_select" />

        <ImageButton
            android:id="@+id/c3"
            android:layout_width="94dip"
            android:layout_height="98dip"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/hy_select" />

        <ImageButton
            android:id="@+id/c4"
            android:layout_width="88dip"
            android:layout_height="88dip"
            android:layout_marginLeft="131dip"
            android:layout_marginTop="16dip"
            android:background="@drawable/bm_select" />

        <ImageButton
            android:id="@+id/c5"
            android:layout_width="88dip"
            android:layout_height="88dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/bx_select" />

        <ImageButton
            android:id="@+id/navball1"
            android:layout_width="110dip"
            android:layout_height="85dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="72dp"
            android:background="@drawable/navball" />
    </RelativeLayout>

</FrameLayout>